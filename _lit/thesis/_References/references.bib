@article{Gizzi.2016,
 abstract = {We discuss analytical and numerical tools for the statistical characterization of the anisotropic strain energy density of soft hyperelastic materials embedded with fibers. We consider spatially distributed orientations of fibers following a tridimensional or a planar architecture. We restrict our analysis to material models dependent on the fourth pseudo-invariant I4 of the Cauchy--Green tensor, and to exponential forms of the fiber strain energy function \textgreek{Y}aniso. Under different loading conditions, we derive the closed-form expression of the probability density function for I4 and \textgreek{Y}aniso. In view of bypassing the cumbersome extension--contraction switch, commonly adopted for shutting down the contribution of contracted fibers in models based on generalized structure tensors, for significant loading conditions we identify analytically the support of the fibers in pure extension. For uniaxial loadings, the availability of the probability distribution function and the knowledge of the support of the fibers in extension yield to the analytical expression of average and variance of I4 and \textgreek{Y}aniso, and to the direct definition of the average second Piola--Kirchhoff stress tensor. For generalized loadings, the dependence of I4 on the spatial orientation of the fibers can be analyzed through angle plane diagrams. Angle plane diagrams facilitate the assessment of the influence of the pure extension condition on the definition of the stable support of fibers for the statistics related to the anisotropic strain energy density.},
 author = {Gizzi, Alessio and Pandolfi, Anna and Vasta, Marcello},
 year = {2016},
 title = {Statistical characterization of the anisotropic strain energy in soft materials with distributed fibers},
 url = {http://www.sciencedirect.com/science/article/pii/S0167663615001945},
 keywords = {Fiber reinforced materials;Fourth pseudo-invariant;Isochoric anisotropic hyperelasticity;Statistical fiber distribution},
 pages = {119--138},
 volume = {92},
 issn = {01676636},
 journal = {Mechanics of Materials},
 doi = {10.1016/j.mechmat.2015.09.008},
 file = {Gizzi, Pandolfi et al. 2016 - Statistical characterization of the anisotropic:E\:\\#Science\\Continuum Mechanics\\#The Literature\\10 Citavi\\Citavi Attachments\\Gizzi, Pandolfi et al. 2016 - Statistical characterization of the anisotropic.pdf:pdf}
}


